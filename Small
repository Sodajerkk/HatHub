				lastid= "http://www.roblox.com/asset/?id=1021627251"
			elseif CurId == 4 then
				lastid= "http://www.roblox.com/asset/?id=224845627"
			elseif CurId == 5 then
				lastid= "http://www.roblox.com/asset/?id=3060405736"
