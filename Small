					SetTween(NeckW,{C0=NeckCF*CFrame.new(0,0,0)*angles(math.rad(65),math.rad(0),math.rad(0))},"Back","Out",0.3)
					SetTween(RW,{C0=CFrame.new(.75 , 0.5, -.0)*angles(math.rad(90),math.rad(0),math.rad(90))},"Back","Out",0.3)
					SetTween(LW,{C0=CFrame.new(-.75, 0.5, -.0)*angles(math.rad(90),math.rad(0),math.rad(-90))},"Back","Out",0.3)
					SetTween(RH,{C0=CFrame.new(.26, 0, -.2)*angles(math.rad(-40),math.rad(0),math.rad(0))},"Back","Out",0.3)
