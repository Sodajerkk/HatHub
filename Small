	function Attack6()

		for i, v in pairs(GatherAllInstances(Effects)) do
			if v.Name == "Zombie" then
				v.Parent:Destroy()
			end
		end
	end

	function ClickCombo()
		if Anim == "Fall" or Anim == "Jump" then
			if Combo == 0 then		
				--DownAT()	
			end
		else
			if agresive == false then				
				if Combo == 0 then
					AT1()	
					Combo = 1
				elseif Combo == 1 then
					AT2()	
					Combo = 2	
				elseif Combo == 2 then
					AT3()
					Combo = 0	
				elseif Combo == 3 then
					Combo = 0	
					ClickCombo()	
				end

			else
				if Combo == 0 then
					AT4()	
					Combo = 1
				elseif Combo == 1 then
					AT5()	
					Combo = 2	
				elseif Combo == 2 then
					AT6()
					Combo = 3	
				elseif Combo == 3 then
					AT7()
					Combo = 0	
				end

			end
		end
	end





